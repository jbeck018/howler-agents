{
  "name": "@howler-agents/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Howler Agents - Group-Evolving Agents",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./generated/*": {
      "import": "./dist/generated/*",
      "types": "./dist/generated/*"
    }
  },
  "files": [
    "dist",
    "src/generated"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.0.0",
    "@connectrpc/connect": "^2.0.0",
    "@connectrpc/connect-node": "^2.0.0",
    "@connectrpc/connect-query": "^2.0.0",
    "@connectrpc/connect-web": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "msw": "^2.12.10",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/howler-agents/howler-agents.git",
    "directory": "packages/howler-agents-ts"
  },
  "homepage": "https://github.com/howler-agents/howler-agents#readme",
  "bugs": "https://github.com/howler-agents/howler-agents/issues",
  "keywords": ["howler-agents", "gea", "agent-evolution", "llm", "sdk"]
}
