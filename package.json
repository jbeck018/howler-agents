{
  "name": "howler-agents",
  "private": true,
  "version": "0.1.0",
  "description": "Group-Evolving Agents - groups of AI agents evolve together by sharing experience across lineages",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r --parallel run dev",
    "dev:ui": "pnpm --filter howler-agents-ui run dev",
    "dev:docs": "pnpm --filter howler-agents-docs run dev",
    "test": "pnpm -r run test",
    "test:ts": "pnpm --filter howler-agents-ts run test",
    "test:ui": "pnpm --filter howler-agents-ui run test",
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck",
    "proto": "./scripts/gen-proto.sh",
    "clean": "pnpm -r run clean"
  }
}
